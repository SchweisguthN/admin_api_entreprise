<div id="developers" class="fr-container--fluid">
  <div class="fr-grid-row">
    <div class="fr-col-12 fr-col-sm-12 fr-col-md-3 fr-col-lg-3 fr-col-xl-3 fr-mt-8v">
      <%= render 'menu' %>
    </div>

    <div class="fr-col-12 fr-col-sm-12 fr-col-md-9 fr-col-lg-9 fr-col-xl-9 fr-background-default-white">
      <%= render 'banner' %>

      <div class="fr-col-12 fr-col-sm-12 card-swagger fr-card fr-enlarge-link">
        <div class="fr-card__body">
          <div class="fr-card__content">
            <h4 class="fr-card__title">
              <%= link_to t('.card_swagger_link'), endpoints_path %>
            </h4>
            <p class="fr-card__desc"><%= t('.card_swagger_description').html_safe %></p>
          </div>
        </div>
      </div>

      <div class="documentation doc_markdown_to_html">
        <% @developers_entries.each do |entry| %>
            <%= raw entry.markdownify %>
        <% end %>
      </div>
    </div>
  </div>
</div>
